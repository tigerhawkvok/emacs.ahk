#!/bin/sh

# This script is provided for backward compatibility with programs
# that use pspell.  Do not use as it will eventually go away.

case $1 in
--version | version)
  echo 0.60.6
  ;;
--datadir | datadir)
  echo "/c/Users/Philip/Downloads/builds/aspell-0.60.6/_inst/share"
  ;;
--pkgdatadir | pkgdatadir)
  echo "/c/Users/Philip/Downloads/builds/aspell-0.60.6/_build/lib/aspell-0.60"
  ;;
*)
  echo "usage: pspell-config version|datadir|pkgdatadir"
  ;;
esac
